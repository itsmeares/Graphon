{
  "name": "graphon",
  "productName": "Graphon",
  "homepage": "https://github.com/Graphon-App/graphon#readme",
  "version": "0.1.9",
  "description": "Graphon - A privacy-focused personal knowledge management and note-taking application",
  "main": "./out/main/index.js",
  "author": "itsmeares",
  "repository": { "type": "git", "url": "git+https://github.com/Graphon-App/graphon.git" },
  "bugs": { "url": "https://github.com/Graphon-App/graphon/issues" },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "tsc -p tsconfig.node.json && electron-vite build",
    "postinstall": "node patch-rebuild.mjs && electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "typecheck": "tsc --noEmit -p tsconfig.json && tsc --noEmit -p tsconfig.node.json",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/utilities": "^3.2.2",
