    "@electron-toolkit/utils": "^4.0.0",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@supabase/supabase-js": "^2.93.3",
    "@tanstack/react-virtual": "^3.13.18",
    "@tiptap/extension-image": "^3.17.0",
    "@tiptap/extension-placeholder": "^3.17.0",
    "@tiptap/extension-task-item": "^3.17.0",
    "@tiptap/extension-task-list": "^3.17.0",
    "@tiptap/extension-typography": "^3.17.0",
    "@tiptap/react": "^3.17.0",
    "@tiptap/starter-kit": "^3.17.0",
    "@tiptap/suggestion": "^3.17.0",
    "@xenova/transformers": "^2.17.2",
    "better-sqlite3": "^12.6.2",
    "chokidar": "^5.0.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "d3-force": "^3.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "electron-updater": "^6.7.3",
    "framer-motion": "^12.29.2",
    "front-matter": "^4.0.2",
    "lucide-react": "^0.563.0",
    "pixi.js": "^8.15.0",
    "react-markdown": "^10.1.0",
    "tailwind-merge": "^3.4.0",
    "tippy.js": "^6.3.7",
    "tiptap-markdown": "^0.9.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.1.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/d3-force": "^3.0.10",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "drizzle-kit": "^0.31.8",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "electron-vite": "^5.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "tar": "^7.5.4"
  }
}
